CREATE TABLE dishpicture (
	id int(11) NOT NULL auto_increment,
	rid int(11)NOT NULL,
	dishid int(11)NOT NULL,
	filename varchar(100) NOT NULL,
	PRIMARY KEY (id)
);

CREATE TABLE tdishpicture (
	id int(11) NOT NULL auto_increment,
	rid int(11)NOT NULL,
	dishid int(11)NOT NULL,
	filename varchar(100) NOT NULL,
	PRIMARY KEY (id)
);

